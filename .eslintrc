{
  "parserOptions": {"ecmaVersion": "latest"},
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "comma-dangle":"error",
    "semi":"error",
    "indent": ["error",2],
    "quotes": ["error","single"],
    "no-trailing-spaces":"error",
    "eol-last":"error",
    "no-multiple-empty-lines":["error", { "max": 0, "maxEOF": 0 }]
  }
}